<div class="form-group container">
    <div class="container-filter">            
        <div class="form-group col-md-12">
            <label class='control-label'>Usuario</label>
            <input type="text" placeholder="Informe o usuario" id="inputUser" class="form-control"/>
        </div>

        <div class="form-group col-md-12">
            <label class='control-label'>Senha</label>
            <input type="password" placeholder="Informe a senha" id="inputPass" class="form-control"/>
        </div>

        <div class="form-group col-md-12">
            <button type="submit" class="btn btn-primary" value="Login" onclick="execLogin()">Login</button>
        </div>
    </div>
</div>